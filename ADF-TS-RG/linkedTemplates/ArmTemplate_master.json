{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-TS-RG"
		},
		"AdvwrkDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdvwrkDB'"
		},
		"BlobStoreProd_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStoreProd'"
		},
		"AdvwrkDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adftusharserver.database.windows.net"
		},
		"AdvwrkDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adf-ts-db"
		},
		"AdvwrkDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Tusharadf"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-TS-RG_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdvwrkDB_password": {
						"value": "[parameters('AdvwrkDB_password')]"
					},
					"BlobStoreProd_connectionString": {
						"value": "[parameters('BlobStoreProd_connectionString')]"
					},
					"AdvwrkDB_properties_typeProperties_server": {
						"value": "[parameters('AdvwrkDB_properties_typeProperties_server')]"
					},
					"AdvwrkDB_properties_typeProperties_database": {
						"value": "[parameters('AdvwrkDB_properties_typeProperties_database')]"
					},
					"AdvwrkDB_properties_typeProperties_userName": {
						"value": "[parameters('AdvwrkDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}